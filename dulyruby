# Temp H

counter = 0

while counter < 5
  puts counter
  counter += 1
end
# Temp I

num = 0

until num == 5
  puts num
  num += 1
end

# Temp J

array = ['five', 'four', 'three', 'two', 'one']

array.each { |element| puts element }

# Temp K

multi_array = [[3, 54], ['Hello ', 'world!'], [true, false]]

multi_array.each {|sub_array| sub_array.each {|var| puts var}}

# Temp L

