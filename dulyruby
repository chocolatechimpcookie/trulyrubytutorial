# Temp S

yelp_reviews = {nori: 3.5,
                mesob: 4.5,
                spice2: 4.0,
                bamboo: 3.0,
                fresco: 2.0}

best_food = yelp_reviews.select {|name, rating| rating > 3}

puts best_food

