# Temp P

fave_lang = {'Keith' => 'Javascript',
             'Erik' => 'Python',
             'Jimmy' => 'PHP',
             'Graeme' => '.NET',
             'Scott' => 'Ruby'}

fave_lang.each {|person, language| puts person + ' loves using ' + language}

