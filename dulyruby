# Temp U

def cube n
  puts n ** 3
end

puts cube(7)

# Temp V

[true, false, true, true, false].each {|bool| puts bool}

# Temp W

drinks = ['red bull', 'monster', 'full-throttle', 'amp', 'rockstar']

drinks.sort! {|drink1, drink2| drink1 <=> drink2}

puts drinks
